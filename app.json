{
  "expo": {
    "name": "Examen Civique France",
    "slug": "examen-civique-france",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "civicexamapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "description": "Application d'aide à la réussite de l'examen civique français",
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#0055A4"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.civicexam.france",
      "buildNumber": "1.0.0"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#0055A4"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.civicexam.france",
      "versionCode": 1
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": ["expo-router", "expo-sqlite", "expo-av", "expo-notifications"],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "your-project-id"
      }
    }
  }
}
